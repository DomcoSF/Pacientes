@model IEnumerable<PacienteNet.Models.Paciente>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomPaciente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomMae)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CPF)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RG)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DtaNascimento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumeroCNS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdFamilia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Casa)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NomPaciente)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomMae)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CPF)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RG)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DtaNascimento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumeroCNS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdFamilia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Casa)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdPaciente }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdPaciente }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdPaciente })
        </td>
    </tr>
}
@Html.ActionLink("Novo", "Create")
</table>
